{
    "name": "drewlabs/component-generators",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0",
        "illuminate/console": "^5.0|^6.0|^7.0|^8.0",
        "drewlabs/database": "^2.0",
        "drewlabs/http": "^2.0",
        "drewlabs/code-generator": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\ComponentGenerators\\": "src/",
            "App\\": "./examples/src/app"
        },
        "files": [
            "./src/proxies.php",
            "./helpers/helpers.php",
            "./helpers/config.php"
        ]
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "asmyns.platonnas29@gmail.com"
        }
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "autoload": {
            "psr-4": {
                "Drewlabs\\ComponentGenerators\\Tests\\": "./tests"
            },
            "files": []
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "drewlabs/contracts": "^2.0.20",
        "drewlabs/core": "^2.0",
        "drewlabs/core-helpers": "^2.0",
        "drewlabs/filesystem": "^1.0",
        "drewlabs/psr7-stream": "^1.0",
        "drewlabs/support": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-code-generator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-filesystem.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-psr7-stream.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-packages-database.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-laravel-http.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-support.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\ComponentGenerators\\Extensions\\Providers\\ServiceProvider"
            ],
            "aliases": {}
        },
        "branch-alias": {
            "dev-master": "v2-dev-branch"
        }
    }
}
