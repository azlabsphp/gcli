{
    "name": "drewlabs/component-generators",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "drewlabs/code-generator": "^2.0",
        "doctrine/dbal": "^3.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drewlabs\\ComponentGenerators\\": "src/",
            "App\\": "./examples/src/app"
        },
        "files": [
            "./src/proxies.php",
            "./helpers.php"
        ]
    },
    "authors": [{
        "name": "azandrew-sidoine",
        "email": "asmyns.platonnas29@gmail.com"
    }],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "autoload": {
            "psr-4": {
                "Drewlabs\\ComponentGenerators\\Tests\\": "./tests"
            },
            "files": []
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^7.4|^8.0|^8.1",
        "drewlabs/core-helpers": "^2.0",
        "drewlabs/filesystem": "^1.0",
        "drewlabs/psr7-stream": "^1.0",
        "drewlabs/php-value": "^0.1.5"
    },
    "repositories": [{
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-code-generator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-filesystem.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-psr7-stream.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-value.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\ComponentGenerators\\Extensions\\Providers\\ServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v2-dev-branch"
        }
    },
    "suggest": {
        "drewlabs/contracts": "Required by generated model, service and view models (^2.5).",
        "drewlabs/support": "Required by generated controllers and services (^2.3)."
    }
}